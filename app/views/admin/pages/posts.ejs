                                                                                    <div class="col-md-9">
                                                                                        <input type="text" class="validate-inp form-control" name="createdby" placeholder="Username" required id="inpUsername">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="col-md-3 control-label">Status<span class="required" aria-required="true"> * </span></label>
