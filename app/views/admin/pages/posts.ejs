                                                                <form class="form-horizontal" role="form" id="formPost">
                                                                    <div class="form-body">
                                                                        <div class="row">
                                                                            <div class="col-xs-12 col-md-6">
                                                                                <div class="form-group">
                                                                                    <label class="col-md-3 control-label">Title<span class="required" aria-required="true"> * </span></label>
                                                                                    <div class="col-md-9">
                                                                                        <input type="text" class="validate-inp form-control" name="title" placeholder="Title" required>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="col-md-3 control-label">Target<span class="required" aria-required="true"> * </span></label>
                                                                                    <div class="col-md-9">
                                                                                        <select class="form-control" name="target">
                                                                                            <option value="1">Male</option>
                                                                                            <option value="2">Female</option>
                                                                                            <option value="3">Both</option>
                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="col-md-3 control-label">Job<span class="required" aria-required="true"> * </span></label>
                                                                                    <div class="col-md-9">
                                                                                        <select class="form-control " name="job">
                                                                                            <option value="0">Sinh viên</option>
                                                                                            <option value="1">Đang đi làm</option>
                                                                                            <option value="2">Đang tìm việc</option>
                                                                                            <option value="3">All</option>
                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="col-md-3 control-label">Description<span class="required" aria-required="true"> * </span></label>
                                                                                    <div class="col-md-9">
                                                                                        <textarea required class="validate-inp form-control" name="description" style="resize: none;" rows="4"></textarea>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
    
                                                                            <div class="col-xs-12 col-md-6">
                                                                                <div class="form-group">
                                                                                    <label class="col-md-3 control-label">City<span class="required" aria-required="true"> * </span></label>
                                                                                    <div class="col-md-9">
                                                                                        <select class="form-control city" name="city">
                                                                                            
                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="col-md-3 control-label">From Age<span class="required" aria-required="true"> * </span></label>
                                                                                    <div class="col-md-9">
                                                                                        <select class="form-control " name="fromage">
                                                                                            <option value="18">18</option>
                                                                                            <option value="19">19</option>
                                                                                            <option value="20">20</option>
                                                                                            <option value="21">21</option>
                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="form-group">
                                                                                    <label class="col-md-3 control-label">To Age<span class="required" aria-required="true"> * </span></label>
